{
  "name": "raider-chat",
  "private": true,
  "displayName": "Raider Chat Extension",
  "description": "Raid repos for code!",
  "version": "1.2.2",
  "engines": {
    "vscode": "^1.92.0"
  },
  "tags": [],
  "categories": [],
  "publisher": "example",
  "main": "extension.js",
  "scripts": {
    "bundle": "webpack --mode production && webpack --mode production --config ./webpack.config.views.js",
    "build": "rm -rf dist && mkdir -p build && npm run bundle && node scripts/build.js",
    "watch": "webpack --mode development --watch",
    "devserver": "webpack serve --mode development --config ./webpack.config.views.js",
    "format": "prettier 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' './package.json' '.swcrc' './*.js' --no-error-on-unmatched-pattern --write"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/node": "^20.8.7",
    "@types/promise-deferred": "^2.0.4",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "@types/uuid": "^9.0.5",
    "@types/vscode": "^1.83.1",
    "@vscode/vsce": "^2.21.1",
    "copy-webpack-plugin": "^11.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "promise-deferred": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "swc-loader": "^0.2.3",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "raider.reset",
        "title": "Reset History",
        "category": "Raider",
        "icon": "$(debug-restart)",
        "group": "Raider"
      },
      {
        "command": "raider.chat",
        "title": "View Chat",
        "category": "Raider",
        "icon": "$(comment-discussion)",
        "group": "Raider"
      },
      {
        "command": "raider.history",
        "title": "View Session History",
        "category": "Raider",
        "icon": "$(history)",
        "group": "Raider"
      },
      {
        "command": "raider.settings",
        "title": "Settings",
        "category": "Raider",
        "icon": "$(settings-gear)",
        "group": "Raider"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "raider.reset",
          "group": "navigation@1",
          "when": "view == raiderChat"
        },
        {
          "command": "raider.chat",
          "group": "navigation@1",
          "when": "view == raiderChat"
        },
        {
          "command": "raider.history",
          "group": "navigation@1",
          "when": "view == raiderChat"
        },
        {
          "command": "raider.settings",
          "group": "navigation@1",
          "when": "view == raiderChat"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "raider",
          "title": "Raider",
          "icon": "static/logo.svg"
        }
      ]
    },
    "views": {
      "raider": [
        {
          "id": "raiderChat",
          "type": "webview",
          "name": "Chat"
        }
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource-variable/inter": "^5.0.21",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "axios": "^1.7.4",
    "raider-chat": "file:"
  }
}
